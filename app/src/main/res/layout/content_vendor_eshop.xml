<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#10000000"
    android:elevation="50dp"
    android:fillViewport="true"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.electonicmarket.android.emarket.Activities.VendorEshop"
    tools:showIn="@layout/activity_vendor_eshop">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/White"
            android:orientation="vertical">



            <com.google.android.material.tabs.TabLayout
                android:id="@+id/vendoreshoptabs"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#10000000"
                app:tabGravity="fill"></com.google.android.material.tabs.TabLayout>

            <com.electonicmarket.android.emarket.Models.Noswipeviewpager
                android:id="@+id/vendoreshopviewpager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#10000000">
            </com.electonicmarket.android.emarket.Models.Noswipeviewpager>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</androidx.core.widget.NestedScrollView>